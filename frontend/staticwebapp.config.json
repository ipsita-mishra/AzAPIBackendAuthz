{
    "routes": [
        {
            "route": "/api/*",
            "methods": [
                "GET"
            ],
            "allowedRoles": [
                "authenticated"
            ],
            "headers": {
                "Cache-Control": "no-store"
            }
        },
        {
            "route": "/login",
            "redirect": "/.auth/login/github?post_login_redirect_uri=.referrer",
            "statusCode": 302
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
        }
    ]
}